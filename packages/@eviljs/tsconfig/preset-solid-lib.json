{
    "extends": [
        "./with-check-strict.json",
        "./with-lib.json"
    ],
    "compilerOptions": {
        "emitDeclarationOnly": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "lib": ["esnext", "dom", "dom.iterable", "webworker"],
        "module": "nodenext",
        "moduleDetection": "auto",
        "moduleResolution": "nodenext",
        "target": "esnext"
    }
}
