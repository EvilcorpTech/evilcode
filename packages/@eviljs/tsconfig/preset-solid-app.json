{
    "extends": [
        "./with-check-strict.json"
    ],
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "lib": ["esnext", "dom", "dom.iterable", "webworker"],
        "module": "esnext",
        "moduleDetection": "auto",
        "moduleResolution": "bundler",
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "target": "esnext"
    }
}
