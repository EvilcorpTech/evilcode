{
    "name": "@eviljs/webpack-html-style-inline-plugin",
    "version": "0.6.0",
    "type": "commonjs",
    "sideEffects": false,
    "files": [
        "/LICENSE.txt",
        "/README.md",
        "/cjs/",
        "/ts/"
    ],
    "main": "./cjs/index.js",
    "scripts": {
        "clean": "rm -rf ./cjs/ || true; rm -rf ./node_modules/.cache/ || true",
        "build": "tsc --listEmittedFiles",
        "prepack": "npm run clean; npm run build",
        "preversion": "npm run build"
    },
    "dependencies": {
        "@types/node": "22",
        "schema-utils": "4",
        "tslib": "2"
    },
    "peerDependencies": {
        "html-webpack-plugin": "5",
        "tapable": "2",
        "webpack": "5"
    },
    "devDependencies": {
        "typescript": "5.7"
    },
    "author": "Daniele Orlando",
    "license": "BSD-2-Clause",
    "description": "Webpack plugin for inlining CSS inside HTML file",
    "keywords": [
        "Webpack",
        "Plugin",
        "Style",
        "CSS",
        "Inline"
    ],
    "homepage": "https://github.com/EvilcorpTech/evilcode",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EvilcorpTech/evilcode.git"
    },
    "bugs": {
        "url": "https://github.com/EvilcorpTech/evilcode/issues"
    }
}
