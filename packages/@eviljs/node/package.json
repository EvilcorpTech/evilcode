{
    "name": "@eviljs/node",
    "version": "1.8.0",
    "type": "module",
    "sideEffects": false,
    "files": [
        "/LICENSE.txt",
        "/esm/",
        "/ts/"
    ],
    "exports": {
        "./*": "./esm/*.js"
    },
    "scripts": {
        "clean": "rm -rf ./esm/ || true; rm -rf ./node_modules/.cache/ || true",
        "build": "tsc --listEmittedFiles",
        "prepack": "npm run clean; npm run build",
        "preversion": "npm run build"
    },
    "dependencies": {
        "@eviljs/std": "1",
        "@types/bcryptjs": "2",
        "@types/node": "22",
        "bcryptjs": "2",
        "tslib": "2"
    },
    "devDependencies": {
        "typescript": "5.7"
    },
    "author": "Daniele Orlando",
    "license": "BSD-2-Clause",
    "description": "Awesome Node",
    "keywords": [
        "Node",
        "TypeScript"
    ],
    "homepage": "https://github.com/EvilcorpTech/evilcode",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EvilcorpTech/evilcode.git"
    },
    "bugs": {
        "url": "https://github.com/EvilcorpTech/evilcode/issues"
    }
}
