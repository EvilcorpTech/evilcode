{
    "type": "module",
    "scripts": {
        "init": "npx pnpm@latest install",
        "upgrade": "npx npm-upgrade@latest && npm run init",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "npm run check-ts && npm run check-circular",
        "check-ts": "npx tsc --noEmit",
        "check-circular": "npx madge@latest --ts-config ./tsconfig.json --circular ./src/main.tsx"
    },
    "dependencies": {
        "@evilcss/std": "1",
        "@evilcss/ui": "1",
        "@eviljs/react": "1",
        "@eviljs/reactx": "1",
        "@eviljs/std": "1",
        "@eviljs/web": "1",
        "@eviljs/webx": "1",
        "prop-types": "15",
        "react": "18",
        "react-dom": "18"
    },
    "devDependencies": {
        "@eviljs/tsconfig": "1",
        "@types/node": "18",
        "@types/prop-types": "15",
        "@types/react": "18",
        "@types/react-dom": "18",
        "@vitejs/plugin-react": "3",
        "postcss": "8",
        "postcss-preset-env": "8",
        "rollup-plugin-visualizer": "5",
        "tslib": "2",
        "typescript": "^4.9",
        "vite": "4",
        "vite-plugin-checker": "0"
    },
    "pnpm": {
        "overrides": {
            "@evilcss/std": "link:../packages/@evilcss/std",
            "@evilcss/ui": "link:../packages/@evilcss/ui",
            "@eviljs/react": "link:../packages/@eviljs/react",
            "@eviljs/reactx": "link:../packages/@eviljs/reactx",
            "@eviljs/std": "link:../packages/@eviljs/std",
            "@eviljs/tsconfig": "link:../packages/@eviljs/tsconfig",
            "@eviljs/web": "link:../packages/@eviljs/web",
            "@eviljs/webx": "link:../packages/@eviljs/webx"
        }
    },
    "engines": {
        "node": ">= 16",
        "npm": ">= 7"
    }
}
